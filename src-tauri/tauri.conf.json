{
  "build": {
    "beforeBuildCommand": "cd src-ui && npm run build",
    "beforeDevCommand": "cd src-ui && npm run dev",
    "devPath": "http://localhost:3000",
    "distDir": "../src-ui/out"
  },
  "package": {
    "productName": "Moras",
    "version": "1.0.1"
  },
  "tauri": {
    "allowlist": {
      "clipboard": {
        "all": false
      },
      "notification": {
        "all": true
      },
      "fs": {
        "readDir": true,
        "createDir": true,
        "exists": true,
        "scope": [
          "**"
        ]
      },
      "dialog": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.moras.as",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 800,
        "resizable": false,
        "label": "moras",
        "title": "Moras",
        "width": 1280
      }
    ]
  }
}
